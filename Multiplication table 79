import java.io.*;
import java.util.*;
public class MultiplicationTable {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        if (n >= 1 && n <= 9) {
            for (int i = 1; i <= n; i++) {
                System.out.println(n + " x " + i + " = " + (n * i));
            }
        } else {
            System.out.print("Invalid Input \n");
        }
    }
}
